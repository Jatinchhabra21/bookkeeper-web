{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": [
        "src/components/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "constants/*": [
        "src/data/constants/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "utils/*": [
        "src/utils"
      ]
    },
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*"
  ]
}